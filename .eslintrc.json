{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "prettier",
        "standard"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "space-before-function-paren": [
            "error",
            "never"
        ],
    }
}
